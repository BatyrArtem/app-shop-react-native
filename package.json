{
  "name": "csnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build-android": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.android.js --bundle-output iOS/main.jsbundle --platform 'android' --assets-dest ./  --dev false --reset-cache",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "extractLangs": "react-gettext-parser --output messages.pot 'src/**/*.js'"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "axios": "0.18.0",
    "base-64": "0.1.0",
    "date-fns": "1.30.1",
    "gettext.js": "git+https://github.com/freiserg/gettext.js.git",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "lodash": "4.17.11",
    "react": "16.3.2",
    "react-apollo": "^2.5.6",
    "react-native": "0.55.4",
    "react-native-actionsheet": "^2.4.2",
    "react-native-device-info": "2.0.1",
    "react-native-extended-stylesheet": "0.11.2",
    "react-native-firebase": "5.3.1",
    "react-native-keyboard-aware-scroll-view": "0.8.0",
    "react-native-navigation": "1.1.478",
    "react-native-payments": "^0.7.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-swipeout": "2.3.6",
    "react-native-swiper": "1.5.14",
    "react-native-vector-icons": "6.4.2",
    "react-redux": "5.1.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-persist": "4.10.1",
    "redux-thunk": "2.3.0",
    "shitty-qs": "1.0.1",
    "tcomb-form-native": "0.6.20"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-native": "4.0.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.13.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "jest": "24.8.0",
    "nock": "10.0.6",
    "react-gettext-parser": "^1.5.0",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "16.8.6",
    "redux-mock-store": "1.5.3"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "setupFiles": [
      "<rootDir>/__tests__/mocks/global.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js?(x)"
    ]
  }
}
